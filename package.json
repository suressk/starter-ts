{
  "name": "pkg-name",
  "version": "0.0.1",
  "packageManager": "pnpm@7.2.1",
  "description": "",
  "author": "K. <sure_ssk@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/suressk/pkg-name#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suressk/pkg-name.git"
  },
  "bugs": "https://github.com/suressk/pkg-name/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "npm run build",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "push": "ts-node scripts/push.ts",
    "release": "ts-node scripts/release.ts"
  },
  "dependencies": {
    "sure-utils": "^1.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.0.0",
    "@types/prompts": "^2.0.14",
    "@types/semver": "^7.3.10",
    "bumpp": "^8.2.1",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.18.0",
    "eslint-config-suressk": "^0.1.0",
    "esno": "^0.16.3",
    "execa": "4.0.2",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "minimist": "^1.2.6",
    "picocolors": "^1.0.0",
    "pnpm": "^7.2.1",
    "prompts": "^2.4.2",
    "rimraf": "^3.0.2",
    "semver": "^7.3.7",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.15.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "ts-node scripts/verify-commit.ts"
    }
  },
  "lint-staged": {
    "*": [
      "eslint . --fix"
    ]
  }
}
